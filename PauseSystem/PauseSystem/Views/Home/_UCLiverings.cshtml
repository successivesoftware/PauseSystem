@model IEnumerable<PauseSystem.Models.CustomerDelivery>

<div class="table-primary">
    <table class="table table-condensed">
        <thead>
            <tr>
                <th>Antal</th>
                <th>Varenr</th>
                <th>Beskrivelse</th>
                <th>Pris</th>
                <th>Samplet pris</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.Antal
                        @*@item.Antal*@
                    </td>
                    <td>
                        @item.VareNr
                        @*@item.Antal*@
                    </td>
                    <td>
                        @item.Beskrivelse
                        @*@item.Antal*@
                    </td>
                    <td>
                        @item.Pris
                        @*@item.Pris*@
                    </td>
                    <td>
                        @item.SampletPris
                        @*@item.Pris*@
                    </td>

                </tr>
            }
        </tbody>
    </table>
</div>